<%= form_for @artist do |f| %>
<fieldset>
  <%= f.label :name %>
  <%= f.text_field :name %>
</fieldset>
<fieldset>
  <%= f.label :nationality %>
  <%= f.text_field :nationality %>
</fieldset>
<fieldset>
  <%= f.label :image %>
  <%= f.url_field :image %>
</fieldset>
<fieldset>
  <%= f.label :dob %>
  <%= f.text_field :dob %>
</fieldset>
<fieldset>
  <%= f.label :instagram %>
  <%= f.text_field :instagram %>
</fieldset>
<fieldset>
  <label></label>
  <%= f.submit %>
</fieldset>
<% end %>
